{
  "name": "@mvsde/website",
  "version": "0.1.0",
  "description": "My personal website.",
  "scripts": {
    "dev:site": "eleventy --input content/site --output dist/site --serve",
    "dev:css": "postcss src/main.css --watch --env development",
    "dev:css:site": "npm run dev:css -- --output dist/site/styles.css",
    "dev:css:twitter": "npm run dev:css -- --output dist/twitter/styles.css",
    "build:site": "eleventy --input content/site --output dist/site --quiet",
    "build:twitter": "eleventy --input content/twitter --output dist/twitter --quiet",
    "build:css": "postcss src/main.css --env production",
    "build:css:site": "npm run build:css -- --output dist/site/styles.css",
    "build:css:twitter": "npm run build:css -- --output dist/twitter/styles.css",
    "lint:js": "eslint \"**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mvsde/website"
  },
  "author": "Fynn Becker <post@fynn.be>",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/mvsde/website/issues"
  },
  "homepage": "https://github.com/mvsde/website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1"
  },
  "dependencies": {
    "modern-normalize": "^0.5.0"
  }
}
