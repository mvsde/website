{
  "name": "@mvsde/website",
  "version": "0.1.0",
  "description": "My personal website.",
  "scripts": {
    "dev:site": "eleventy --input content --output dist --serve",
    "dev:css": "postcss src/main.css --watch --env development --output dist/styles.css",
    "build:site": "eleventy --input content --output dist --quiet",
    "build:css": "postcss src/main.css --env production --output dist/styles.css",
    "lint:js": "eslint \"**/*.js\"",
    "lint:language": "alex content/**/*.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mvsde/website"
  },
  "author": "Fynn Becker <post@fynn.be>",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/mvsde/website/issues"
  },
  "homepage": "https://github.com/mvsde/website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "alex": "^8.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "got": "^11.3.0",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1"
  }
}
