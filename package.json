{
  "name": "@mvsde/website",
  "version": "0.1.0",
  "description": "My personal website.",
  "scripts": {
    "dev:site": "eleventy --input content/site --output dist/site --serve",
    "dev:css:site": "postcss src/main.css --watch --env development --output dist/site/styles.css",
    "dev:css:twitter": "postcss src/twitter.css --watch --env development --output dist/twitter/styles.css",
    "build:site": "eleventy --input content/site --output dist/site --quiet",
    "build:twitter": "eleventy --input content/twitter --output dist/twitter --quiet",
    "build:css:site": "postcss src/main.css --env production --output dist/site/styles.css",
    "build:css:twitter": "postcss src/twitter.css --env production --output dist/twitter/styles.css",
    "lint:js": "eslint \"**/*.js\"",
    "lint:language": "alex content/**/*.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mvsde/website"
  },
  "author": "Fynn Becker <post@fynn.be>",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/mvsde/website/issues"
  },
  "homepage": "https://github.com/mvsde/website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "alex": "^8.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1"
  },
  "dependencies": {
    "modern-normalize": "^0.6.0"
  }
}
